      // initialize waypoints for section-headings
      $('.section-heading').waypoint(function(direction) {
        if (direction === 'down') $(this.element).addClass('fadeInUp');
        else $(this.element).removeClass('fadeInUp');
      }, { offset: '96%' });
      $('#company-caption').waypoint(function(direction) {
        if (direction === 'down') $(this.element).addClass('bounceInLeft');
        else $(this.element).removeClass('bounceInLeft');
      }, { offset: '96%' });
      $('#grad-hat').waypoint(function(direction) {
        if (direction === 'up') $(this.element).addClass('fadeInDown');
        else $(this.element).removeClass('pulse');
      }, { offset: '96%' });

      $('#company-title>span').each(function(){
