  $(function() {
      $(window).on('load', function(){
        $(document.body).addClass('loading-done');
      });
      // Start of Tawk.to Script--
      var Tawk_API=Tawk_API||{}, Tawk_LoadStart=new Date();
	(function(){
		var s1=document.createElement("script"),s0=document.getElementsByTagName("script")[0];
		s1.async=true;
		s1.src='https://embed.tawk.to/583fdcaf8a20fc0cac4a75ee/default';
		s1.charset='UTF-8';
		s1.setAttribute('crossorigin','*');
		s0.parentNode.insertBefore(s1,s0);
	})();
	// Start of Tawk.to Script
      //= shrink and expand the navbar 
      $(window).bind('scroll', function () {
      	var wScroll = $(this).scrollTop();
      	//console.log(wScroll);
      	if ($(window).scrollTop() > 5) 
      	$('#grad-hat img').addClass("flyhat");
      	else 
      		$('#grad-hat img').removeClass("flyhat");
      	// $('#grad-hat img').css({'transform': 'translate(0px, -'+wScroll*2+'%)'});
      	// $('#company-title').css({'transform': 'translate(0px, '+wScroll+'%)'});
      	// $('#company-caption').css({'transform': 'translate(0px, '+wScroll/wScroll+'%)'});
        if ($(window).scrollTop() > 50) 
        {
        	$("#change-container").removeClass("container-fluid");
        	$("#change-container").addClass("container");        	
        	$('.navbar').addClass('shrink');
        	$("div.navbar-left").addClass("navbar-left-tab")
        	$('#ck-logo').addClass('transit');
        	$('.rect').addClass('logo-transit');
        	$('.rect').removeClass('breath');
        	$('.rect1').addClass('logo-content-transit1');
        	$('.rect2').addClass('logo-content-transit2');
        	$('.rect3').addClass('logo-content-transit3');
        	// $('#company-caption').css({'transform': 'translate(0px,'+100*wScroll/wScroll+'%)'});

        }
        else
        {
        	
        	$("#change-container").removeClass("container");
        	$("#change-container").addClass("container-fluid");
	        $('.navbar').removeClass('shrink');
	        $('#ck-logo').removeClass('transit');
	        $('.rect').removeClass('logo-transit');
	        $('.rect').addClass('breath');
			$('.rect1').removeClass('logo-content-transit1');
			$('.rect2').removeClass('logo-content-transit2');
			$('.rect3').removeClass('logo-content-transit3');
    	 }
      	// if ($(window).scrollTop() > 678) 
      	// {
      	// 	$('#slide-nav').addClass('show-nav');
      	// 	$('#slide-nav').removeClass('hide-nav');
      	// }
      	// else
      	// 	$('#slide-nav').removeClass('show-nav');
      	// 	$('#slide-nav').addClass('hide-nav');
      });

      //= autoplay the video when the modal show up
      

      //= equal columns height
      // matchHeight('#states .col-inner');

      //= counterUp plugin
      $('.counterUp').counterUp({ delay: 10, time: 1500 });

      //= set the max-height property for .navbar-collapse
      $(window).on('load resize orientationchange', function(){
        $('.navbar-collapse').css('max-height', $(window).height() - $('.navbar').height());
      });

      $(document).on('click', '[data-toggle="collapse"]', function(e){
        var $trigger = $(e.target);
        $trigger.is('[data-toggle="collapse"]') || ($trigger = $trigger.parents('[data-toggle="collapse"]'));
        var $target = $($trigger.attr('data-target'));
        if ($target.attr('id') === 'page-menu-collapse')
          $(document.body).toggleClass('navbar-collapse-show', !$trigger.hasClass('collapsed'))
      });

      //= initialize smooth scroll plugin
      smoothScroll.init({
	        offset: 60,
	        speed: 1000,
	        updateURL: false
	      });
